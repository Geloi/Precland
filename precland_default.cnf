#default config file
[algorithm]
eccentricity = 0.8
min_radius = 9
area_ratio = 0.8
min_ring_ratio =  0.70
max_ring_ratio = 0.90


[logging]
location = ~/precland/
print_level = debug, general, algorithm
log_level = aircraft , algorithm, general
display_level = raw, gui
record_level = raw, gui
record_type = image

[camera]
source = 0
background_capture = False
has_gimbal = True
horizontal-fov = 94.3
vertical-fov = 55.0
matrix = 614.01269552 0.0 315.00073982
	0.0 614.43556296 237.14926858
	0.0 0.0 1.0

distortion = 0.12269303 -0.26618881 0.00129035 0.00081791 0.17005303


[simulator]
use_simulator = True
width = 640
height = 640
frame_rate = 5
target_size = 2
target_location = ~/precland/Targets/mat_v1.jpg

[general]
always_run = True

[land_control]
operation_mode = velocity
init_dec_speed = 1.0
final_dec_speed = 0.5
init_dec_alt = 20
goto_distance = 1.5
final_dec_alt = 2
abort_alt = 5
landed_alt = 0.5
input_scalar = 1.0
update_rate = 10
